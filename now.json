{
  "builds": [
        { "src": "*.js", "use": "@now/node" },
		{ "src": "api/*.php", "use": "now-php@0.0.9" }
  ],
  "routes": [
    { "src": "/api/v2/RKS-(?<type>[^/]+)",  "dest": "/api/get.php?type=$type&mode=RKS" },
	{ "src": "/api/v2/RKS-(?<type>[^/]+)/json",  "dest": "/api/get.php?type=$type&mode=RKS&format=json" },
    { "src": "/(.*)",  "dest": "/woop.js" }
  ]
}